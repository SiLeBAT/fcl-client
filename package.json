{
  "name": "fcl-web-app",
  "version": "1.4.0",
  "license": "GPL v3",
  "scripts": {
    "ng": "ng",
    "audit": "better-npm-audit audit",
    "serverlessstart": "ng serve --configuration=serverless",
    "serverlessstart:wra": "ng serve --host 0.0.0.0 --configuration=serverless",
    "start": "ng serve --proxy-config proxy.config.json",
    "serve": "ng serve --proxy-config proxy.config.json --watch --hmr",
    "serve:fcldev": "ng serve --proxy-config proxy.config.fcldev.json --watch",
    "serve:local": "ng serve",
    "build:qa": "npm run clean && ng build --configuration=qa --output-path \"public\" && npm run postbuild",
    "build:production": "npm run clean && ng build --configuration=production --output-path \"public\" && npm run postbuild",
    "build:serverlessprod": "npm run clean && ng build --configuration=serverlessprod --output-path \"public\" && npm run postbuild",
    "postbuild": "node build-utils/post-build-minify-json \"public/**/*.json\"",
    "test": "jest",
    "test:detect-open-handles": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:update-snapshot": "jest --updateSnapshot",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint:code": "npx ng lint",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:styles:fix": "stylelint --fix \"src/**/*.scss\"",
    "gh-pages": "ng build --configuration=production --bh /fcl-client/",
    "cloud9": "ng serve --host $IP --port $PORT",
    "clean": "npx rimraf public/*"
  },
  "private": true,
